{
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "plugins": ["import"],
    "extends": ["eslint:recommended", "airbnb", "airbnb/hooks"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "import/first": "error",
        "react/jsx-filename-extension": [1, { "allow": "always" }]
    }
}
